<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>JspAppWelcome</display-name> 
  <servlet>
  	<servlet-name>controller</servlet-name>
  	<servlet-class>com.nt.controller.MainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>controller</servlet-name>
  	<url-pattern>/mainServlet</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>model</servlet-name>
  	<servlet-class>com.nt.models.BookBean</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>model</servlet-name>
  	<url-pattern>/model</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>service</servlet-name>
  	<servlet-class>com.nt.service.DBConnector</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>service</servlet-name>
  	<url-pattern>/service</url-pattern>
  </servlet-mapping>
</web-app>